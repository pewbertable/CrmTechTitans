{
    "ConnectionStrings": {
        "DefaultConnection": "Data Source=CrmDatabase.db"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "IdentityOptions": {
        "SignIn": {
            "RequireConfirmedAccount": false
        },
        "Password": {
            "RequireDigit": false,
            "RequireLowercase": false,
            "RequireUppercase": false,
            "RequireNonAlphanumeric": false,
            "RequiredLength": 3
        }
    }
}
